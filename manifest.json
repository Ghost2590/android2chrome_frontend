{
    "name": "Android2Chrome",
    "version": "0.9",
    "manifest_version": 2,
    "background": {
        "page": "app/modules/background/background.html"
    },
    "browser_action": {
        "default_icon": "images/icons/icon_32.png",
        "default_popup": "app/modules/popup/popup.html"
    },
    "icons": {
        "128": "images/icons/icon_128.png",
        "16": "images/icons/icon_32.png",
        "32": "images/icons/icon_32.png",
        "48": "images/icons/icon_48.png"
    },
    "content_security_policy": "script-src 'self'; object-src 'self'",
    "permissions": [ "http://*/*", "https://*/*" , "tabs", "storage"],
    "options_page": "app/modules/options/options.html"
}
